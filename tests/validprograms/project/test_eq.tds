Program {
	
	void assertTrue(Bool expression) extern;
	
	void assertFalse(Bool expression) extern;
	
	void testEQ_1() {
		Bool b = 1 == 0;
		assertFalse(b);
	}
	
	void testEQ_2() {
		Bool b = 0 == 1;
		assertFalse(!b);
	}
	
	void testEQ_3() {
		Bool b = 1 == 1;
		assertTrue(b);
	}
	
	void testEQ_4() {
		Bool b = -1 == 0;
		assertFalse(b);
	}
	
	void testEQ_5() {
		Bool b = -2 == 1;
		assertFalse(b);
	}
	
	void testEQ_6() {
		Bool b = 1 == -1;
		assertFalse(b);
	}

	void main() {
		testEQ_1();
		testEQ_2();
		testEQ_3();
		testEQ_4();
		testEQ_5();
		testEQ_6();
	}
}
