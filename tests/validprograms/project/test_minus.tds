Program {
	void print(Int x) extern;
	
	void assertTrue(Bool expression) extern;
	
	void testMinus_1() {
		Int x = -1;
		assertTrue(x == -1);
	}
	
	void testMinus_2() {
		Int x = -2;
		assertTrue(x == -2);
	}
	
	void testMinus_3() {
		Int x = -(1 * 2);
		assertTrue(x == -2);
	}
	
	void testMinus_4() {
		Int x = --1;
		assertTrue(x == 1);
	}
	
	void testMinus_5() {
		Int x = -(-1);
		assertTrue(x == 1);
	}
	
	void testMinus_6() {
		Int x0 = -1;
		Int x1 = -2;
		Int x2 = -(x0 + x1);
		assertTrue(x2 == 3);
	}
	
	void testMinus_7() {
		Int x0 = -1;
		Int x1 = -2;
		Int x2 = -(x0 - x1);
		assertTrue(x2 == -1);
	}

	void main() {
		testMinus_1();
		testMinus_2();
		testMinus_3();
		testMinus_4();
		testMinus_5();
		testMinus_6();
		testMinus_7();
	}
}
