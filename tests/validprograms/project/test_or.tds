Program {	
	void assertTrue(Bool expression) extern;
	
	void testOr_1() {
		Bool b = True || True;
		assertTrue(b);
	}
	
	void testOr_2() {
		Bool b = True || False;
		assertTrue(b);
	}
	
	void testOr_3() {
		Bool b = False || True;
		assertTrue(b);
	}
	
	void testOr_4() {
		Bool b = False || False;
		assertTrue(b == False);
	}
	
	void testOr_5() {
		Bool b = True || True || True || False;
		assertTrue(b);
	}
	
	void testOr_6() {
		Bool b = False || False || False || True;
		assertTrue(b);
	}
	
	void testOr_7() {
		Bool b = False || False || False || False;
		assertTrue(b == False);
	}

	void main() {
		testOr_1();
		testOr_2();
		testOr_3();
		testOr_4();
		testOr_5();
		testOr_6();
		testOr_7();
	}
}
