Program {
    void print(Int x) extern;
    
    void newline() extern;

    Int foo(Int x, Int y, Int z) {
        return x + y - z;
    }

    void print_bool(Bool b) extern;
    
    void main() {
        Bool b_1 = True;	// expected: 1
        Bool b_2 = !False;	// expected: 1
        Bool b_3 = !b_1;	// expected: 0
        Bool b_4 = !b_3;	// expected: 1

        print_bool(b_1); // True
        newline();
        print_bool(b_2); // True
        newline();
        print_bool(b_3); // False
        newline();
        print_bool(b_4); // True
        newline();
    }
}
