Program {
	void assertTrue(Bool expression) extern;
	
	void assertFalse(Bool expression) extern;
	
	void testLT_1() {
		Bool b = 1 < 0;
		assertFalse(b);
	}
	
	void testLT_2() {
		Bool b = 0 < 1;
		assertTrue(b);
	}
	
	void testLT_3() {
		Bool b = 1 < 1;
		assertFalse(b); 
	}
	
	void testLT_4() {
		Bool b = -1 < 0;
		assertTrue(b);
	}
	
	void testLT_5() {
		Bool b = -2 < 1;
		assertTrue(b);
	}
	
	void testLT_6() {
		Bool b = 1 < -1;
		assertFalse(b);
	}

	void main() {
		testLT_1();
		testLT_2();
		testLT_3();
		testLT_4();
		testLT_5();
		testLT_6();
	}
}
