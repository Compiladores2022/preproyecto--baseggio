Program {

void assertTrue(Bool expression) extern;

void assertEquals(Int expected, Int value) extern;

void print(Int x) extern;

void testSubstraction_1() {
	Int x1 = 1 - 0;
	assertEquals(1, x1);
}

void testSubstraction_2() {
	Int x1 = 0 - 1;
	assertEquals(-1, x1);
}

void testSubstraction_3() {
	Int x1 = 1 - 0;
	Int x2 = 0 - 1;
	Int x3 = x1 - x2;
	assertEquals(2, x3);
}

void testSubstraction_4() {
	Int x1 = 1 - 0;
	Int x2 = 0 - 1;
	Int x3 = x2 - x1;
	assertEquals(-2, x3);
}

void testSubstraction_5() {
	Int x5 = ((((5 - 4) - 3) - 2) - 1);
	assertEquals(-5, x5);
}

void testSubstraction_6() {
	Int x6 = ((((1 - 2) - 3) - 4) - 5);
	assertEquals(-13, x6);
}

void testSubstraction_7() {
	Int x1 = 1 - 0;
	Int x2 = 0 - 1;
	Int x3 = x1 - x2;

	Int x4 = x2 - x1;
	Int x5 = ((((5 - 4) - 3) - 2) - 1);
	Int x6 = ((((1 - 2) - 3) - 4) - 5);
	Int x7 = ((((x1 - x2) - x3) - x4) - x5) - x6;
	assertEquals(20, x7);
}

void testSubstraction_8() {
	Int x1 = 1 - 0;
	Int x2 = 0 - 1;
	Int x3 = x1 - x2;

	Int x4 = x2 - x1;
	Int x5 = ((((5 - 4) - 3) - 2) - 1);
	Int x6 = ((((1 - 2) - 3) - 4) - 5);
	Int x7 = ((((x6 - x5) - x4) - x3) - x2) - x1;
	assertEquals(-8, x7);
}

void main() {
	testSubstraction_1();
	testSubstraction_2();
	testSubstraction_3();
	testSubstraction_4();
	testSubstraction_5();
	testSubstraction_6();
	testSubstraction_7();
	testSubstraction_8();
}

}
