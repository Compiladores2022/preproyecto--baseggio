Program {	
	void assertTrue(Bool expression) extern;
	
	void assertFalse(Bool expression) extern;
	
	void testGT_1() {
		Bool b = 1 > 0;
		assertTrue(b);
	}
	
	void testGT_2() {
		Bool b = 0 > 1;
		assertFalse(b);
	}
	
	void testGT_3() {
		Bool b = 1 > 1;
		assertFalse(b);
	}
	
	void testGT_4() {
		Bool b = -1 > 0;
		assertFalse(b);
	}
	
	void testGT_5() {
		Bool b = -2 > 1;
		assertFalse(b);
	}
	
	void testGT_6() {
		Bool b = 1 > -1;
		assertTrue(b);
	}

	void main() {
		testGT_1();
		testGT_2();
		testGT_3();
		testGT_4();
		testGT_5();
		testGT_6();
	}
}
