Program {

	void print(Int x) extern;
	
	void assertTrue(Bool expression) extern;
	
	void testDivision_1() {
		Int x = 1 / 2;
		assertTrue(x == 0);
	}
	
	void testDivision_2() {
		Int x = 2 / 1;
		assertTrue(x == 2);
	}
	
	void testDivision_3() {
		Int x = 2 / 2;
		assertTrue(x == 1);
	}
	
	void testDivision_4() {
		Int x = 0 / 2;
		assertTrue(x == 0);
	}
	
	void testDivision_5() {
		Int x = 6 / 3 / 2;
		assertTrue(x == 1);
	}
	
	void testDivision_6() {
		Int x = 2 / 3 / 6;
		assertTrue(x == 0);
	}
	
	void testDivision_7() {
		Int x = 100 / 2 / 5 / 2 / 5;
		assertTrue(x == 1);
	}

	void main() {
		testDivision_1();
		testDivision_2();
		testDivision_3();
		testDivision_4();
		testDivision_5();
		testDivision_6();
		testDivision_7();
	}

}
