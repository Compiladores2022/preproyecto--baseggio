Program {

void print(Int x) extern;

void assertTrue(Bool expression) extern;

void assertEquals(Int expected, Int value) extern;

void testAddition_1() {
	Int x1 = 0 + 1;
	assertEquals(1, x1);
}

void testAddition_2() {
	Int x1 = 1 + 0;
	assertEquals(1, x1);
}

void testAddition_3() {
	Int x1 = 0 + 1;
	Int x2 = x1 + 1;
	assertEquals(2, x2);
}

void testAddition_4() {
	Int x1 = 1 + 0;
	Int x2 = 1 + x1;
	assertEquals(2, x2);
}

void testAddition_5() {
	Int x1 = 0 + 1 + 2 + 3;
	assertEquals(6, x1);
}

void testAddition_6() {
	Int x1 = 0 + 1;
	Int x2 = 1 + 0;
	Int x3 = x1 + 1;
	Int x4 = 1 + x2;
	Int x5 = x1 + x2 + x3 + x4;
	assertEquals(6, x5);
}

void testAddition_7() {
	Int x1 = 0 + 1;
	Int x2 = 1 + 0;
	Int x3 = 0 + x1 + 1 + x2;
	assertEquals(3, x3);
}

void main() {
	testAddition_1();
	testAddition_2();
	testAddition_3();
	testAddition_4();
	testAddition_5();
	testAddition_6();
	testAddition_7();
}

}
